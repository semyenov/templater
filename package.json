{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@sozdev/templater",
  "type": "module",
  "version": "0.0.5",
  "packageManager": "yarn@3.5.1",
  "description": "Templater CLI",
  "author": "Alexander Semyenov <semyenov@hotmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/semyenov/templater#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/semyenov/templater.git"
  },
  "bugs": "https://github.com/semyenov/templater/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "templater": "bin/templater-cli.mjs"
  },
  "scripts": {
    "dev": "esno src/index.ts",
    "build": "rollup --configPlugin typescript --config rollup.config.ts",
    "typecheck": "tsc --noEmit",
    "start": "node --experimental-modules --experimental-import-meta-resolve dist/esm/index.mjs",
    "schema": "typescript-json-schema --out schema.json --skipLibCheck src/config.ts Config",
    "release": "yarn npm publish",
    "lint": "eslint --ext=ts,js,vue .",
    "clean": "yarn exec rm -rf dist"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@antfu/eslint-config-ts": "^0.39.8",
    "@antfu/utils": "^0.7.5",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/esm": "^3.2.0",
    "@types/js-yaml": "^4",
    "@types/node": "^20.4.2",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "esbuild": "^0.18.14",
    "eslint": "^8.45.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-antfu": "^0.39.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsonc": "^2.9.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unicorn": "^48.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.15.1",
    "eslint-plugin-yml": "^1.8.0",
    "esno": "^0.17.0",
    "jsonc-eslint-parser": "^2.3.0",
    "lint-staged": "^13.2.3",
    "rollup": "^3.26.3",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "simple-git-hooks": "^2.8.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "typescript-json-schema": "^0.59.0",
    "vue-eslint-parser": "^9.3.1",
    "yaml-eslint-parser": "^1.2.2"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint --fix"
  },
  "dependencies": {
    "@clack/prompts": "^0.6.3",
    "change-case": "^4.1.2",
    "consola": "^3.2.3",
    "dotenv": "^16.3.1",
    "glob": "^10.3.3",
    "handlebars": "^4.7.7",
    "js-yaml": "^4.1.0",
    "picocolors": "^1.0.0",
    "zod": "^3.21.4"
  }
}
